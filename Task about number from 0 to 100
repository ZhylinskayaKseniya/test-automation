class Dev {
  int dev[] = new int[101];
  public void enter() {
    for (int i = 0; i <= 100; i++) {
      dev[i] = i;
    }
  }
  public void show() {
    for (int i = 0; i <= 100; i++) {
      if(dev[i] % 3 == 0) {
        System.out.print("3*" + dev[i]/3 + "\n");
      }
        if(dev[i] % 3 != 0) {
	  System.out.print(dev[i] + " ");
	}
     }
  }
  public static void main(String[] args) {
    Dev d = new Dev();
    d.enter();
    d.show();   
  }
}
